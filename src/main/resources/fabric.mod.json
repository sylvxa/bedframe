{
	"schemaVersion": 1,
	"id": "bedframe",
	"version": "${version}",
	"name": "Bedframe",
	"description": "Library mod that handles textured/custom server-side Polymer blocks across both Geyser and Java clients.",
	"authors": [
		"sylvxa"
	],
	"contact": {
		"homepage": "https://sylvie.lol/",
		"sources": "https://github.com/sylvxa/bedframe"
	},
	"license": "CC0-1.0",
	"icon": "assets/bedframe/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"lol.sylvie.bedframe.BedframeInitializer"
		]
	},
	"mixins": [
		"bedframe.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
